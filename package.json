{
	"name": "cards",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "./node_modules/.bin/ava && ./node_modules/.bin/tsc --target esnext --moduleResolution node --noEmit tests/*.ts && ./node_modules/.bin/eslint .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.35.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.1",
		"@types/cookie": "^0.5.1",
		"@types/node": "^20.4.1",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"ava": "^5.3.1",
		"carbon-components-svelte": "^0.76.0",
		"carbon-icons-svelte": "^11.4.0",
		"carbon-preprocess-svelte": "^0.9.1",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^4.16.2",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.5",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.0",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@envelop/core": "^4.0.0",
		"@envelop/graphql-jit": "^6.0.1",
		"@graphql-yoga/common": "^3.9.1",
		"@graphql-yoga/node": "^3.9.1",
		"@graphql-yoga/render-graphiql": "^4.0.3",
		"@pothos/core": "^3.30.0",
		"@pothos/plugin-prisma": "^3.52.0",
		"@prisma/client": "^4.16.2",
		"ayy": "^3.0.0",
		"blake3-wasm": "^3.0.0",
		"graphql": "^16.7.1",
		"postgres": "^3.3.5"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=tsx"
		]
	},
	"pnpm": {
		"overrides": {
			"esbuild": "^0.17.18",
			"blake3-wasm": "^3.0.0",
			"@c4312/blake3-internal": "^3.0.0"
		},
		"patchedDependencies": {
			"@graphql-yoga/render-graphiql@4.0.3": "patches/@graphql-yoga__render-graphiql@4.0.3.patch"
		}
	}
}
