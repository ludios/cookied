{
	"name": "cards",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "./node_modules/.bin/ava && ./node_modules/.bin/tsc --target esnext --moduleResolution node --noEmit tests/*.ts && ./node_modules/.bin/eslint .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.34.3",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.0",
		"@types/cookie": "^0.5.1",
		"@types/node": "^18.16.16",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"ava": "^5.3.0",
		"carbon-components-svelte": "^0.72.3",
		"carbon-icons-svelte": "^11.4.0",
		"carbon-preprocess-svelte": "^0.9.1",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^4.15.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.5.2",
		"tsx": "^3.12.7",
		"typescript": "^4.9.5",
		"vite": "^4.3.9"
	},
	"type": "module",
	"dependencies": {
		"@envelop/core": "^3.0.6",
		"@envelop/graphql-jit": "^5.0.6",
		"@graphql-yoga/common": "^2.12.12",
		"@graphql-yoga/node": "^2.13.13",
		"@graphql-yoga/render-graphiql": "^2.13.12",
		"@pothos/core": "^3.30.0",
		"@pothos/plugin-prisma": "^3.51.1",
		"@prisma/client": "^4.15.0",
		"ayy": "^3.0.0",
		"blake3-wasm": "^3.0.0",
		"graphql": "^16.6.0",
		"postgres": "^3.3.4"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nodeArguments": [
			"--loader=tsx"
		]
	},
	"pnpm": {
		"patchedDependencies": {
			"@graphql-yoga/render-graphiql@2.13.12": "patches/@graphql-yoga__render-graphiql@2.13.12.patch"
		},
		"overrides": {
			"esbuild": "^0.17.18",
			"blake3-wasm": "^3.0.0",
			"@c4312/blake3-internal": "^3.0.0"
		}
	}
}
