{
	"name": "cards",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "NODE_OPTIONS='--loader=tsx --no-warnings' ./node_modules/.bin/ava && ./node_modules/.bin/tsc --target esnext --moduleResolution node --noEmit tests/*.ts && ./node_modules/.bin/eslint .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.36.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.4",
		"@types/cookie": "^0.5.1",
		"@types/node": "^20.4.8",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"ava": "^5.3.1",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.32.4",
		"prettier": "^3.0.1",
		"prettier-plugin-svelte": "^3.0.3",
		"prisma": "^5.1.1",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.1",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"vite": "^4.4.8"
	},
	"type": "module",
	"dependencies": {
		"@envelop/core": "^4.0.0",
		"@envelop/graphql-jit": "^6.0.1",
		"@graphql-yoga/common": "^3.9.1",
		"@graphql-yoga/node": "^3.9.1",
		"@graphql-yoga/render-graphiql": "^4.0.3",
		"@pothos/core": "^3.32.1",
		"@pothos/plugin-prisma": "^3.56.0",
		"@prisma/client": "^5.1.1",
		"ayy": "^3.0.0",
		"graphql": "^16.7.1",
		"hash-wasm": "^4.9.0",
		"postgres": "^3.3.5"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		}
	},
	"pnpm": {
		"overrides": {
			"esbuild": "^0.18.17"
		},
		"patchedDependencies": {
			"@graphql-yoga/render-graphiql@4.0.3": "patches/@graphql-yoga__render-graphiql@4.0.3.patch"
		}
	}
}
