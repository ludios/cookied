{
	"name": "cards",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "NODE_OPTIONS='--loader=tsx --no-warnings' ./node_modules/.bin/ava && ./node_modules/.bin/tsc --target esnext --moduleResolution node --noEmit tests/*.ts && ./node_modules/.bin/eslint .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.39.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.27.1",
		"@types/cookie": "^0.5.3",
		"@types/node": "^20.8.9",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"ava": "^5.3.1",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.34.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prisma": "^5.5.2",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.2",
		"tsx": "^3.14.0",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	},
	"type": "module",
	"dependencies": {
		"@envelop/core": "^5.0.0",
		"@envelop/graphql-jit": "^8.0.1",
		"@graphql-yoga/common": "^3.9.1",
		"@graphql-yoga/node": "^3.9.1",
		"@graphql-yoga/render-graphiql": "^4.0.5",
		"@pothos/core": "^3.38.0",
		"@pothos/plugin-prisma": "^3.61.0",
		"@prisma/client": "^5.5.2",
		"ayy": "^3.0.0",
		"graphql": "^16.8.1",
		"hash-wasm": "^4.10.0",
		"postgres": "^3.4.2"
	},
	"ava": {
		"extensions": {
			"ts": "module"
		}
	},
	"pnpm": {
		"overrides": {
			"esbuild": "^0.19.2"
		}
	}
}
